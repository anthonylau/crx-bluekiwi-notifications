{
  "manifest_version": 2,

  "name": "BlueKiwi ZEN Notifications for myatos.net",
  "description": "This is an unofficial extension for Atos Staff for use with BlueKiwi",
  "version": "0.3.3.2",

  "background": {
    "scripts": [
		"common.js",
		"jquery.js",
		"background.js"
	]
  },
  
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  
  "browser_action": {
	  "default_icon": {                    
      "19": "icon48.png",          
      "38": "icon48.png"          
    },
	"default_popup": "popup.html",
	"default_title": "BlueKiwi Notifier"
  },
  "permissions": [
	"https://zen.myatos.net/notification*",
	"notifications"
  ],
  
  "web_accessible_resources": [
    "bluekiwi.ico"
  ],
  
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}